{
    "name": "django-html-2",
    "version": "1.0.0",
    "license": "MIT",
    "description": "Django template language support for Visual Studio Code",
    "displayName": "Django Template",
    "categories": [
        "Languages",
        "Snippets",
        "Linters"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "contributes": {
        "grammars": [
            {
                "language": "html",
                "scopeName": "text.html.django",
                "path": "./syntaxes/django-html.json"
            }
        ],
        "snippets": [
            {
                "language": "html",
                "scopeName": "text.html.django",
                "path": "./snippets/django-snippets.json"
            }
        ]
    }
}